{
    "manifest_version": 3,
    "name": "Site Blocker",
    "description": "Productivity extension used to block sites",
    "permissions": [
        "webRequest",
	    "webRequestBlocking", 
        "<all_urls>"
    ],
    "version": "0.0.1",
    "icons": {
      "16": "icons/icon16.png",
      "18": "icons/icon18.png"
    },
    "content_scripts": [
      {
        "js": ["content.js"],
        "matches": ["<all_urls>"]
      }
    ]
}